/**index.wxss**/
.swiper-container{
    width: 100%;
    height: 100%;
}
.swiper-slide{
	width: 100%;
    height: 100%;
    background: #1b1b1f;
    overflow: hidden;
	position: relative;
}
.swiper-slide .banner{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    object-fit: cover;
}
.swiper-slide .swiper-mask{
    width: 100%;
    height: 100%;
    position: relative;
    background: -webkit-linear-gradient(top,rgba(0,0,0,.01) 0,rgba(0,0,0,.1) 70%,rgba(0,0,0,.3) 100%);
    overflow: hidden;
}
.swiper-slide .swiper-mask .author-info{
    position: absolute;
    left: 0;
    bottom: 10px;
    width: 100%;
    color: #fff;
}
.swiper-slide .swiper-mask .author-info .ad{
    display: inline-block;
    font-size: 12px;
    color: #fff;
    height: 20px;
    line-height: 21px;
    padding: 0 5px 0 6px;
    background-color: #008dea;
    border-radius: 4px;
    margin-left: 15px;
    margin-bottom: 3px;
}
.swiper-slide .swiper-mask .author-info .name{
    font-size: 16px;
    font-weight: normal;
    line-height: 32px;
    padding: 0 15px;
}
.swiper-slide .swiper-mask .author-info .desc{
    font-size: 14px;
    line-height: 20px;
    padding: 0 15px;
}
.side-bar{
    position: absolute;
    top: 50%;
    right: -20px;
    font-size: 10px;
    color: #fff;
    line-height: 10px;
    text-align: center;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
@-webkit-keyframes spin {
    0% {
        -webkit-transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(360deg);
    }
}
@keyframes spin {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(360deg);
        transform: rotate(360deg);
    }
}
.side-bar .ico-figure{
    display: block;
    width: 50px;
    height: 50px;
    margin: 0 auto;
    border-radius: 50%;
    box-sizing: border-box;
    animation:spin linear 5s infinite;
    -webkit-animation:spin linear 5s infinite;
}
.side-bar .ico-heart{
    display: block;
    width: 100px;
    height: 100px;
    margin-top: -10px;
    margin-bottom: -25px;
    background: url(data:image/png;base64,) no-repeat;
    background-position: left;
    background-size: 2900%;
}
@-webkit-keyframes heartBlast {
    0% {
        background-position: left;
    }
    100% {
        background-position: right;
    }
}
@keyframes heartBlast {
    0% {
        background-position: left;
    }
100% {
        background-position: right;
    }
}
.side-bar .heartAnimation {
    display: inline-block;
    -webkit-animation-name: heartBlast;
    animation-name: heartBlast;
    -webkit-animation-duration: .8s;
    animation-duration: .8s;
    -webkit-animation-iteration-count: 1;
    animation-iteration-count: 1;
    -webkit-animation-timing-function: steps(28);
    animation-timing-function: steps(28);
    background-position: right;
}

.swiper-slide .swiper-mask .video-play{
    position: absolute;
    width: 150rpx;
    height: 150rpx;
    left: 50%;
    top: 50%;
    margin-left: -75rpx;
    margin-top: -75rpx;
    background: url(data:image/png;base64,) no-repeat;
    background-position: left bottom;
    background-size: 100% auto;
}
