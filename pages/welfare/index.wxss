/**index.wxss**/
.swiper-container{
    width: 100%;
    height: 100%;
}
.swiper-banner{
    width: 100%;
    height: 360rpx;
    background-color: #fff;
}
.swiper-slide{
    width: 100%;
    height: 100%;
    background: #fff;
    overflow: hidden;
    position: relative;
}
.swiper-slide .banner{
    width: 100%;
    height: 100%;
}

.welfare-announcement{
    display: block;
    height: 56px;
    padding: 8px 15px 8px 60px;
    box-sizing: border-box;
    background: url(data:image/jpeg;base64,) #fff no-repeat;
    background-size: 40px;
    background-position: 10px center;
}
.welfare-announcement .swiper-slide .title{
    overflow: hidden;
    white-space: nowrap;
    font-size: 13px;
    color: #3d464d;
    line-height: 20px;
}
.welfare-announcement .swiper-slide .tag{
    color: #ea6f5a;
    padding-right: 10px;
}

.welfare-subject{
    margin-top: 15px;
    background-color: #fff;
    padding: 0 15px;
}
.welfare-subject-grids{
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    position: relative;
}
.welfare-subject-grids:after{
    content: '';
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    border-bottom: 1px solid #f0f0f0;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.welfare-subject-grids .item{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    position: relative;
    line-height: 70px;
    text-align: center;
    font-family: PingFang SC Bold;
    font-size: 14px;
    font-weight: bold;
    color: #333;
}
.welfare-subject-grids .item.mid{
    -webkit-box-flex: 1.1;
    -webkit-flex: 1.1;
    flex: 1.1;
}
.welfare-subject-grids .item.mid:before{
    content: '';
    position: absolute;
    left: 0;
    top: 50%;
    width: 1px;
    height: 30px;
    background-color: #f1f1f1;
    margin-top: -15px;
    -webkit-transform-origin: center center;
    transform-origin: center center;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.welfare-subject-grids .item.mid:after{
    content: '';
    position: absolute;
    right: 0;
    top: 50%;
    width: 1px;
    height: 30px;
    background-color: #f1f1f1;
    margin-top: -15px;
    -webkit-transform-origin: center center;
    transform-origin: center center;
    -webkit-transform: scaleY(0.5);
    transform: scaleY(0.5);
}
.welfare-subject-grids .item .first{
    padding-right: 15px;
}
.welfare-subject-grids .item .last{
    padding-left: 15px;
}
.welfare-subject-grids .item .user{
    display: inline-block;
    width: 18px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    vertical-align: -4px;
    margin-right: 8px;
}
.welfare-subject-grids .item .fire{
    display: inline-block;
    width: 18px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    vertical-align: -4px;
    margin-right: 8px;
}
.welfare-subject-grids .item .crown{
    display: inline-block;
    width: 18px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    vertical-align: -4px;
    margin-right: 8px;
}
.welfare-subject-grids .item .hot{
    position: absolute;
    left: 48px;
    top: 6px;
    width: 32px;
    height: 18px;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
}
.welfare-subject-topic{
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    padding: 12px 0;
}
.welfare-subject-topic .brand{
    display: block;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    position: relative;
    padding-top: 22px;
    box-sizing: border-box;
    background: url(data:image/png;base64,) #fbfbfb no-repeat;
    background-position: right bottom;
    background-size: 100% auto;
}
.welfare-subject-topic .brand .title{
    position: absolute;
    top: 0;
    left: 10px;
    font-size: 10px;
    color: #fff;
    background-color: #ff8c65;
    height: 21px;
    line-height: 22px;
    padding: 0 6px;
}
.welfare-subject-topic .brand-flex{
    width: 54%;
    margin-left: 5px;
}
.welfare-subject-topic .domain{
    display: block;
    height: 70px;
    position: relative;
    margin-bottom: 5px;
    padding-top: 22px;
    box-sizing: border-box;
    background: url(data:image/png;base64,) #fbfbfb no-repeat;
    background-position: right center;
    background-size: auto 80%;
}
.welfare-subject-topic .domain .title{
    position: absolute;
    top: 0;
    left: 10px;
    font-size: 10px;
    color: #fff;
    background-color: #ffc557;
    height: 21px;
    line-height: 22px;
    padding: 0 6px;
}
.welfare-subject-topic .mark{
    display: block;
    height: 70px;
    position: relative;
    padding-top: 22px;
    box-sizing: border-box;
    background: url(data:image/png;base64,) #fbfbfb no-repeat;
    background-position: right center;
    background-size: auto 100%;
}
.welfare-subject-topic .mark .title{
    position: absolute;
    top: 0;
    left: 10px;
    font-size: 10px;
    color: #fff;
    background-color: #4dacf0;
    height: 21px;
    line-height: 22px;
    padding: 0 6px;
}
.welfare-subject-topic .h2{
    font-family: PingFang SC Bold;
    font-size: 15px;
    font-weight: bold;
    color: #333;
    padding-left: 10px;
}
.welfare-subject-topic .h3{
    font-family: PingFang SC;
    font-size: 11px;
    font-weight: normal;
    color: #999;
    padding-left: 10px;
}

.welfare-item{
    display: block;
    margin: 15px 15px 0 15px;
    border-radius: 4px;
    background-color: #fff;
}
.welfare-item .banner{
    width: 100%;
    height: 345rpx;
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
}
.welfare-item-bottom{
    display: -webkit-box;
    display: -moz-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    justify-content: space-between;
}
.welfare-item-title{
    line-height: 47px;
    padding-left: 10px;
    font-size: 16px;
    color: #333;
}
.welfare-item-time{
    line-height: 47px;
    padding-right: 15px;
    font-size: 13px;
    color: #b2b2b8;
}